-- Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local ClickSound = Instance.new("Sound")
ClickSound.SoundId = "rbxassetid://535716488"
ClickSound.Volume = 1
ClickSound.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
function PlayClick()
    ClickSound:Play()
end

local Window = Rayfield:CreateWindow({
    Name = "MozCraft",
    Icon = 95704992992208,
    LoadingTitle = "MozCraft: RoScript",
    LoadingSubtitle = "SzR Studio",
    ShowText = "Creative",
    Theme = "AmberGlow",
    ToggleUIKeybind = "Z",
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,
    ConfigurationSaving = {Enabled = true, FolderName = nil, FileName = "Hub"},
    Discord = {Enabled = true, Invite = "hJDraXq5ks", RememberJoins = true},
    KeySystem = false
})

-- Info Tab
local InfoTab = Window:CreateTab("Info", 14563650667)
InfoTab:CreateButton({
    Name = "Discord",
    Callback = function()
        PlayClick()
        setclipboard("https://discord.gg/hJDraXq5ks")
        Rayfield:Notify({Title = "Discord Invitation", Content = "Copied!", Duration = 2})
    end,
})
InfoTab:CreateParagraph({Title = "Updates & Announcements ðŸ“°", Content = "No Bug & Fixes Found."})

-- Construction Tab
local ConstructionTab = Window:CreateTab("Construction", 88202572338513)
local function CreateButton(Name, URL)
    ConstructionTab:CreateButton({
        Name = Name,
        Callback = function()
            PlayClick()
            loadstring(game:HttpGet(URL))()
        end,
    })
end

