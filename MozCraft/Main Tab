-- Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local ClickSound = Instance.new("Sound")
ClickSound.SoundId = "rbxassetid://535716488"
ClickSound.Volume = 1
ClickSound.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
function PlayClick()
    ClickSound:Play()
end

-- Create Window
local Window = Rayfield:CreateWindow({
    Name = "MozCraft",
    Icon = 95704992992208, 
    LoadingTitle = "MozCraft: RoScript",
    LoadingSubtitle = "SzR Studio",
    ShowText = "Creative",
    Theme = "AmberGlow",
    ToggleUIKeybind = "Z",
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,

    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil, 
        FileName = "Hub"
    },

    Discord = {
        Enabled = true, 
        Invite = "hJDraXq5ks",
        RememberJoins = true
    },

    KeySystem = false,  
    KeySettings = {
        Title = "MozCraft ",
        Subtitle = "Key System",
        Note = "No method of obtaining the key is provided",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Key_41414"}  
    }
})

-- Info Tab
local Tab = Window:CreateTab("Info", 14563650667)

--Info Buttons
local Button = Tab:CreateButton({
   Name = "Discord",
   Interact = Copy,
   Callback = function()
     PlayClick() 
     setclipboard("https://discord.gg/hJDraXq5ks") 
     Rayfield:Notify({Title = "Discord Invitation", Content = "Copied! ", Duration = 2})
   end,
})
local Paragraph = Tab:CreateParagraph({Title = "Updates & Announcements ðŸ“¢", Content = "No Bug & Fixes Found. "})

